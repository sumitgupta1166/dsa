// Initial Template for C++
#include <bits/stdc++.h>
using namespace std;


// } Driver Code Ends
// User function Template for C++
class Solution {
  public:
    string armstrongNumber(int n) {
          int num=n;     
        int sum=0;
        while(num>0){
             int ld=num%10;
             sum+=(ld*ld*ld);    
             num=num/10;
    }
    if(sum==n){
    return "true";
    }    
    return "false";
    // code here
    }
};

//{ Driver Code Starts.
int main() {
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        Solution ob;
        cout << ob.armstrongNumber(n) << endl;
    }
    return 0;
}

// } Driver Code Ends
