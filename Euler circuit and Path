//{ Driver Code Starts
#include<bits/stdc++.h>
using namespace std;

// } Driver Code Ends
class Solution {
public:
	int isEulerCircuit(int V, vector<int>adj[]){
	     int even_degree=0;
        int odd_degree=0;
        
        for(int i=0;i<V;i++){
            if(adj[i].size()%2==0){
                even_degree++;
            }else{
                odd_degree++;
            }
        }
        
        if(even_degree==V){
            return 2;
        }
        
        if(odd_degree>0 && odd_degree==2)
        return 1;
        
        return 0;// Code here
	}
};

//{ Driver Code Starts.
int main(){
	int tc;
	cin >> tc;
	while(tc--){
		int V, E;
		cin >> V >> E;
		vector<int>adj[V];
		for(int i = 0; i < E; i++){
			int u, v;
			cin >> u >> v;
			adj[u].push_back(v);
			adj[v].push_back(u);
		}
		Solution obj;
		int ans = obj.isEulerCircuit(V, adj);
		cout << ans <<"\n";	}
	return 0;
}
// } Driver Code Ends
